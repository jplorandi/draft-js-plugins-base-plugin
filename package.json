{
  "name": "draft-js-base-plugin",
  "version": "0.0.4",
  "description": "A base draft-js plugin to create your own plugins",
  "main": "src/index.js",
  "author": {
    "name": "Juan Pablo Lorandi",
    "email": "jp@cfyar.com",
    "url": "http://www.cfyar.com"
  },
  "keywords": [
    "draft-js",
    "plugins",
    "draft-js-plugins",
    "images"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/jplorandi/draft-js-plugins-base-plugin.git"
  },
  "scripts": {
    "build": "webpack --mode=build",
    "pub": "webpack --mode=build && webpack --mode=dev && npm publish",
    "dev": "webpack --progress --colors --watch --mode=dev",
    "example-build": "cd examples && webpack --mode=build",
    "example-dev": "cd examples && webpack --progress --colors --watch --mode=dev",
    "test": "mocha --compilers js:babel-core/register --colors -w ./test/*.spec.js"
  },
  "license": "MIT",
  "devDependencies": {
    "babel-core": "^6.14.0",
    "babel-eslint": "^6.1.2",
    "babel-loader": "^6.2.5",
    "babel-polyfill": "^6.13.0",
    "babel-preset-es2015": "^6.14.0",
    "babel-preset-react": "^6.11.1",
    "babel-preset-stage-0": "^6.5.0",
    "chai": "^3.5.0",
    "chai-properties": "^1.2.1",
    "css-loader": "^0.24.0",
    "draft-js-entity-props-plugin": "^1.0.2",
    "draft-js-richbuttons-plugin": "^1.1.0",
    "eslint": "^3.4.0",
    "eslint-loader": "^1.5.0",
    "html-webpack-plugin": "^2.22.0",
    "mocha": "^3.0.2",
    "path": "^0.12.7",
    "react": "^15.3.1",
    "react-dom": "^15.3.1",
    "uuid": "^2.0.2",
    "webpack": "^1.13.2",
    "yargs": "^5.0.0"
  },
  "dependencies": {
    "draft-js": "^0.8.1",
    "draft-js-ast-exporter": "0.0.3",
    "draft-js-plugins-editor": "^2.0.0-beta.2",
    "immutable": "^3.8.1",
    "loglevel": "^1.4.1",
    "react-decorate": "^5.1.1"
  }
}
